<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="340.0" prefWidth="377.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hhuebner.autogp.controllers.RoomEditorController">
   <children>
      <VBox prefHeight="340.0" prefWidth="377.0">
         <children>
            <TextField maxWidth="-Infinity" prefHeight="25.0" prefWidth="253.0" text="Name" />
            <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="367.0" spacing="5.0">
               <children>
                  <Label text="Minimale Größe" />
                  <TextField prefHeight="25.0" prefWidth="85.0" fx:id="sizeField" />
                  <ChoiceBox prefHeight="25.0" prefWidth="36.0" fx:id="sizeUnitChoice" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="51.0" prefWidth="367.0" spacing="5.0">
               <children>
                  <Label text="Typ:">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </Label>
                  <ChoiceBox prefWidth="150.0" />
               </children>
            </HBox>
            <ListView maxWidth="-Infinity" prefHeight="123.0" prefWidth="288.0" />
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="140.0" spacing="50.0">
               <children>
                  <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="120.0" text="Ok" onAction="#onFinish" />
                  <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="119.0" text="Abbrechen" onAction="#onCancel" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets left="10.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
